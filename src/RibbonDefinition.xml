    <Application.Views>

        <Ribbon>

            <Ribbon.ApplicationMenu>
                <ApplicationMenu CommandName="FileMenu">

                    <ApplicationMenu.RecentItems>
                        <RecentItems CommandName="MRUList" MaxCount="16" EnablePinning="false" />
                    </ApplicationMenu.RecentItems>

                    <MenuGroup Class="MajorItems">
                        <Button CommandName="New" />

                        <SplitButton CommandName="MenuOpen">
                            <SplitButton.ButtonItem>
                                <Button CommandName="Open" />
                            </SplitButton.ButtonItem>
                            <SplitButton.MenuGroups>
                                <MenuGroup Class="MajorItems">
                                    <Button CommandName="OpenProject" />
                                </MenuGroup>
                            </SplitButton.MenuGroups>
                        </SplitButton>

                        <SplitButton CommandName="MenuImport">
                            <SplitButton.ButtonItem>
                                <Button CommandName="Import" />
                            </SplitButton.ButtonItem>
                            <SplitButton.MenuGroups>
                                <MenuGroup Class="MajorItems">
                                    <Button CommandName="ImportFile" />
                                </MenuGroup>
                            </SplitButton.MenuGroups>
                        </SplitButton>

                        <SplitButton CommandName="MenuExport">
                            <SplitButton.ButtonItem>
                                <Button CommandName="Export" />
                            </SplitButton.ButtonItem>
                            <SplitButton.MenuGroups>
                                <MenuGroup Class="MajorItems">
                                    <Button CommandName="ExportFile" />
                                    <Button CommandName="ExportData" />
                                    <Button CommandName="ExportToExcel" />
                                </MenuGroup>
                            </SplitButton.MenuGroups>
                        </SplitButton>

                        <Button CommandName="Save"  />
                        <Button CommandName="SaveAs"  />
                    </MenuGroup>

                </ApplicationMenu>
            </Ribbon.ApplicationMenu>

            <Ribbon.HelpButton>
                <HelpButton CommandName="Help" />
            </Ribbon.HelpButton>

            <Ribbon.QuickAccessToolbar>
                <QuickAccessToolbar CommandName="QAT">
                    <QuickAccessToolbar.ApplicationDefaults>
                        <Button CommandName="Save" ApplicationDefaults.IsChecked="true" />
                    </QuickAccessToolbar.ApplicationDefaults>
                </QuickAccessToolbar>
            </Ribbon.QuickAccessToolbar>

            <Ribbon.Tabs>
                <Tab CommandName="TabHome">
                    <Tab.ScalingPolicy>
                        <ScalingPolicy>
                            <ScalingPolicy.IdealSizes>
                                <Scale Group="GroupEdit" Size="Medium" />
                                <Scale Group="GroupModel" Size="Large" />
                                <Scale Group="GroupViews" Size="Large" />
                            </ScalingPolicy.IdealSizes>
                            <Scale Group="GroupModel" Size="Medium" />
                            <Scale Group="GroupViews" Size="Medium" />
                        </ScalingPolicy>
                    </Tab.ScalingPolicy>

                    <Group CommandName="GroupEdit" SizeDefinition="FourButtons">
                        <Button CommandName="Properties" />
                        <Button CommandName="Clone" />
                        <Button CommandName="Rename" />
                        <Button CommandName="Remove" />
                    </Group>

                    <Group CommandName="GroupViews" SizeDefinition="FourButtons">
                        <ToggleButton CommandName="InputFile" />
                        <ToggleButton CommandName="SourceTable" />
                        <Button CommandName="ReceptorEditor" />
                        <Button CommandName="WindRose" />
                    </Group>

                    <Group CommandName="GroupModel" SizeDefinition="ThreeButtons">
                        <Button CommandName="Validate" />
                        <Button CommandName="Run" />
                        <Button CommandName="Analyze" />
                    </Group>

                    <Group CommandName="GroupShow">
                        <SizeDefinition>
                            <ControlNameMap>
                                <ControlNameDefinition Name="ShowProjectBrowser" />
                                <ControlNameDefinition Name="ShowValidation" />
                                <ControlNameDefinition Name="ShowModelOutput" />
                            </ControlNameMap>
                            <GroupSizeDefinition Size="Large">
                                <ControlSizeDefinition ControlName="ShowProjectBrowser" ImageSize="Small" IsLabelVisible="true" />
                                <ControlSizeDefinition ControlName="ShowValidation" ImageSize="Small" IsLabelVisible="true" />
                                <ControlSizeDefinition ControlName="ShowModelOutput" ImageSize="Small" IsLabelVisible="true" />
                            </GroupSizeDefinition>
                            <GroupSizeDefinition Size="Medium">
                                <ControlSizeDefinition ControlName="ShowProjectBrowser" ImageSize="Small" IsLabelVisible="true" />
                                <ControlSizeDefinition ControlName="ShowValidation" ImageSize="Small" IsLabelVisible="true" />
                                <ControlSizeDefinition ControlName="ShowModelOutput" ImageSize="Small" IsLabelVisible="true" />
                            </GroupSizeDefinition>
                            <GroupSizeDefinition Size="Small">
                                <ControlSizeDefinition ControlName="ShowProjectBrowser" ImageSize="Small" IsLabelVisible="true" />
                                <ControlSizeDefinition ControlName="ShowValidation" ImageSize="Small" IsLabelVisible="true" />
                                <ControlSizeDefinition ControlName="ShowModelOutput" ImageSize="Small" IsLabelVisible="true" />
                            </GroupSizeDefinition>
                        </SizeDefinition>
                        <CheckBox CommandName="ShowProjectBrowser" />
                        <CheckBox CommandName="ShowValidation" />
                        <CheckBox CommandName="ShowModelOutput" />
                    </Group>
                </Tab>
            </Ribbon.Tabs>

            <Ribbon.ContextualTabs>
                <TabGroup CommandName="TabGroupSourceTools">
                    <Tab CommandName="TabSourceTools">
                        <Tab.ScalingPolicy>
                            <ScalingPolicy>
                                <ScalingPolicy.IdealSizes>
                                    <Scale Group="GroupSourceTools" Size="Medium" />
                                    <Scale Group="GroupSelect" Size="Medium" />
                                </ScalingPolicy.IdealSizes>
                            </ScalingPolicy>
                        </Tab.ScalingPolicy>

                        <Group CommandName="GroupSourceTools" SizeDefinition="FiveButtons">
                            <DropDownButton CommandName="AddSource">
                                <MenuGroup>
                                    <Button CommandName="AddAreaSource" />
                                    <Button CommandName="AddCircularSource" />
                                    <Button CommandName="AddPolygonSource" />
                                </MenuGroup>
                            </DropDownButton>
                            <Button CommandName="EditGeometry" />
                            <Button CommandName="Resample" />
                            <Button CommandName="PlotFluxProfile" />
                            <Button CommandName="Color" />
                            <!--<DropDownColorPicker CommandName="Color" ColorTemplate="ThemeColors" />-->
                        </Group>

                        <Group CommandName="GroupSelect" SizeDefinition="ThreeButtons">
                            <Button CommandName="SelectAll" />
                            <Button CommandName="SelectNone" />
                            <Button CommandName="SelectInverse" />
                        </Group>
                    </Tab>
                </TabGroup>
            </Ribbon.ContextualTabs>

        </Ribbon>

    </Application.Views>

</Application>
